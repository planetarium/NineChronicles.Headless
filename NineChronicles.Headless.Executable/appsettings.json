{
    "Serilog": {
        "Using": [ "Serilog.Expressions" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate":
                        "[{Timestamp:HH:mm:ss} {Level:u3}] {Source}{Message:lj}{NewLine}{Exception}"
                }
            }
        ],
        "Filter": [
            {
                "Name": "ByExcluding",
                "Args": {
                    "expression": "SourceContext = 'Libplanet.Stun.TurnClient'"
                }
            }
        ]
    }
}
